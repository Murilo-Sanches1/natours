{
  "name": "natours",
  "version": "1.0.0",
  "description": "Landing Page, Natours - Float Layout",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass ./sass/main.scss ./css/style.min.css -w",
    "compile:sass": "node-sass ./sass/main.scss ./css/style.comp.css",
    "concat:sass": "concat -o ./css/style.concat.css ./css/icons.css ./css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' ./css/style.concat.css -o ./css/style.prefix.css",
    "compress:css": "node-sass ./css/style.prefix.css ./css/style.min.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:sass prefix:css compress:css"
  },
  "author": "Murilo Sanches",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concat": "^1.0.3",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0"
  }
}
